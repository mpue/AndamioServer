<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="org.pmedv.pojos.plugins.Faq" table="FAQ">
		
		<id name="id" column="FAQ_ID">
                <generator class="increment"/>
        </id>
			
		<property name="studyId" column="STUDY_ID"/>
		<property name="question" column="QUESTION" type="text"/>	
		<property name="answer" column="ANSWER" type="text" length="2147483647"/>	
	 	      							
	</class>
	
</hibernate-mapping>