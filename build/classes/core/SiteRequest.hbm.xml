<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="org.pmedv.pojos.SiteRequest" table="SITEREQUEST">
		
		<id name="id" column="REQUEST_ID" >
			<generator class="increment" />
		</id>
		
		<property name="username" column="USERNAME"/>
		<property name="contentname" column="CONTENTNAME" />		
		<property name="reqtime"  column="REQTIME"/>
		<property name="userip" column="USERIP"/>
		<property name="useragent" column="USERAGENT"/>
		<property name="domain" column="DOMAIN"/>

	</class>
	
</hibernate-mapping>